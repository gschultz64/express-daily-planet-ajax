<h1>Articles</h1>
<br/>
<a href="/articles/new" class="btn btn-primary">Add New Article</a>

<% articles.forEach(function(article, idx) { %>
  <div class="well">
    <a href="/articles/<%= idx %>"><br/><h2><%= article.title %></h2></a><a href="/articles/<%= article.id %>" id="delete">Delete</a>
  </div>
<% }); %>
